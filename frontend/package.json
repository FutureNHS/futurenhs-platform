{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--require ./tracing.js' next build",
    "start": "NODE_OPTIONS='--require ./tracing.js' next start",
    "test": "jest",
    "lint": "yarn eslint",
    "clean": "rm -rf .next"
  },
  "dependencies": {
    "@azure/monitor-opentelemetry-exporter": "^1.0.0-preview.2",
    "@opentelemetry/api": "^0.9.0",
    "@opentelemetry/core": "^0.9.0",
    "@opentelemetry/node": "^0.9.0",
    "@opentelemetry/plugin-http": "^0.9.0",
    "@opentelemetry/plugin-https": "^0.9.0",
    "@opentelemetry/tracing": "^0.9.0",
    "async-hooks": "^1.3.1",
    "next": "9.4.1",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@testing-library/react": "^10.0.4",
    "@types/node": "^14.0.4",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.0.0",
    "eslint-plugin-react": "^7.20.0",
    "ignore-loader": "^0.1.2",
    "jest": "^26.0.1",
    "typescript": "^3.9.3"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "./node_modules/.bin/eslint --fix"
      ],
      "*.{ts,tsx}": [
        "yarn lint"
      ]
    }
  }
